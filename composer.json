{
  "name": "emresari89/laravel-options",
  "description": "Global options loaded from the database",
  "keywords": [
    "emresari89",
    "laravel-options"
  ],
  "homepage": "https://github.com/emresari89/laravel-options",
  "license": "MIT",
  "authors": [
    {
      "name": "Emre Sarıoğlu",
      "email": "admin@emresarioglu.com.tr",
      "homepage": "https://emresarioglu.com.tr",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2.5",
    "illuminate/support": "*",
    "illuminate/database": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
  },
  "require-dev": {
    "orchestra/database": "^4.0|^5.0|^6.0|^9.0|^10.0|^11.0",
    "orchestra/testbench": "^4.0|^5.0|^6.0|^9.0|^10.0|^11.0"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Emresari\\Options\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Emresari\\Options\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Emresari\\Options\\OptionsServiceProvider"
      ],
      "aliases": {
        "Option": "Emresari\\Options\\OptionFacade"
      }
    }
  }
}
